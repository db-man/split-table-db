[
  {
    "name": "users",
    "columns": [
      {
        "id": "userId",
        "name": "User ID",
        "type": "NUMBER",
        "primary": true,
        "filter": true,
        "type:randomPage": [
          "ImageLink",
          "{\"url\":\"{{record.photos.[0]}}\",\"imgSrc\":\"{{record.photos.[0]}}\"}"
        ]
      },
      { "id": "name", "name": "Name", "type": "STRING", "filter": true },
      {
        "id": "roleCode",
        "name": "Role Code",
        "type": "STRING",
        "referenceTable": "roles",
        "enum": ["maintainer", "developer"]
      },
      {
        "id": "maleOrFemale",
        "name": "Male or Female",
        "type": "STRING",
        "type:createUpdatePage": "RadioGroup",
        "enum": ["male", "female"]
      },
      { "id": "age", "name": "Age", "type": "NUMBER" },
      { "id": "active", "name": "Active", "type": "BOOL" },
      { "id": "tags", "name": "Tags", "type": "STRING_ARRAY" },
      {
        "id": "notes",
        "name": "Notes",
        "type": "STRING",
        "type:createUpdatePage": "TextArea"
      },
      {
        "id": "photos",
        "name": "Photos",
        "type": "STRING_ARRAY",
        "type:listPage": [
          "ImageLinks",
          "{\"imgs\":[{{#each record.photos}}{{#if @index}},{{/if}}{\"url\":\"{{this}}\",\"imgSrc\":\"{{#replace \".jpg\" \".jpg_400x400.jpg\"}}{{this}}{{/replace}}\"}{{/each}}],\"limit\":3}"
        ],
        "type:getPage": [
          "PhotoList",
          "{\"photos\":[{{#each record.photos}}{{#if @index}},{{/if}}{\"url\":\"{{this}}\",\"imgSrc\":\"{{this}}_400x400.jpg\"}{{/each}}]}"
        ],
        "type:createUpdatePage": "MultiLineInputBox",
        "filter": true,
        "isListPageImageViewKey": true
      }
    ],
    "large": true
  },
  {
    "name": "roles",
    "columns": [
      {
        "id": "code",
        "name": "Code",
        "type": "STRING",
        "primary": true
      },
      { "id": "name", "name": "Name", "type": "STRING" }
    ]
  }
]
